<script type="text/javascript" src="js/jquery.min.js"></script>
<script type="text/javascript" src="js/bootstrap.js"></script>
<script type="text/javascript" src="js/jquery.mCustomScrollbar.concat.min.js"></script>
<script type="text/javascript" src="js/app.js"></script>
<script type="text/javascript">
$(function(){
	$("#leftMain").load("iframe3.html");
	$("#rightMain").attr("src", "iframe16.html");
})

function _M(menuid,targetUrl) {
	$("#leftMain").load(targetUrl);
	// $("#rightMain").attr("src", targetUrl);
	$(".nLi").removeClass("on");
	$("#_M"+menuid).addClass("on");
	
	// 显示左侧菜单，当点击顶部时，展开左侧
	$(".g-sd").removeClass("left_menu_on");
	$("#openClose").removeClass("close");
	$("html").removeClass("on");
	$("#openClose").data("clicknum", 0);
}
function _MP(menuid,targetUrl){
	$("#rightMain").attr("src", targetUrl);
	$(".submenu").removeClass("on");
	$("#_MP"+menuid).addClass("on");
}

// 自定义滚动条
(function($){
	$(window).load(function(){
		$("#Scroll").mCustomScrollbar({theme:"minimal-dark"});
	});
})(jQuery);

// 调用浏览器窗口
function openwin(openHtml,i,j){
	window.open(""+openHtml+"", "newwindow", "width="+i+", height="+j+", toolbar=no, menubar=no, scrollbars=no, resizable=no, location=no, status=no, top=200, left=200");
}
</script>
